<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">


<mapper namespace="com.minse0.spring.test.ajax.repository.FavoriteRepository">

    <insert id="insert" parameterType="com.minse0.spring.test.ajax.domain.Favorite">
        INSERT INTO favorite (name, url)
        VALUES (#{name}, #{url})
    </insert>

    <select id="selectAll" resultType="com.minse0.spring.test.ajax.domain.Favorite">
        SELECT id, name, url FROM favorite
    </select>

</mapper>
 